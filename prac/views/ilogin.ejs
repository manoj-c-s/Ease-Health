<%- include("./partials/head") %>
    <div class="row mt-5">
        <div class="col-md-6 m-auto">
          <div class="card card-body" style=" background: unset;">
            <h1 class="text-center mb-3"><i class="fas fa-sign-in-alt"></i> Instructor Login</h1>
            <%- include ("./partials/messages") %>
            <form action="/users/ilogin" method="POST">
              <div class="form-group">
              <label for="email">Email</label>
              <input
              type="email"
              id="email"
              name="email"
              class="form-control"
              placeholder="Enter Email"
              />
            </div>
            <div class="form-group">
              <label for="password">Password</label>
              <input
              type="password"
              id="password"
              name="password"
              class="form-control"
              placeholder="Enter Password"
              />
            </div>
            <button type="submit" class="btn btn-primary btn-block" >Login</button>
          </form>
         <a href="/" class="login">User login</a>
         
        </div>
      </div>
       </div>
